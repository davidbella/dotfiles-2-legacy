{
  "app/models/*.rb": {
    "alternate": "test/models/{}_test.rb",
    "type": "model"
  },
  "test/models/*_test.rb": {
    "alternate": "app/models/{}.rb",
    "type": "test"
  },
  "app/controllers/*.rb": {
    "alternate": "test/controllers/{}_test.rb",
    "type": "controller"
  },
  "test/controllers/*_test.rb": {
    "alternate": "app/controllers/{}.rb",
    "type": "test"
  }
}
